+//    Breadcrumb
+  },
+
+  data:function () {
+    return {
+      defaultList: [
+        {
+          label: '主页',
+          link: 'index'
+        }
+      ]
+    }
+  },
+
+  computed: {
+    name () {
+      return this.$route.name || this.$route.title
+    },
+
+    list () {
+      return this.defaultList.concat(this.find(this.name, this.menu))
+    }
+  },
+
+  methods: {
+    find (name, list) {
+      let temp = []
+      for (let i = 0, l = list.length; i < l; ++i) {
+        let item = list[i]
+        if (item.label === name || item.link === name) {
+          temp.push({
+            label: item.label,
+            link: item.link
+          })
+          break
+        }
+
+        if (item.subMenu) {
+          if (item.link) {
+            temp.push({
+              lable: item.label,
+              link: item.link
+            })
+          }
+          let e = this.find(name, item.subMenu)
+          if (e.length) {
+            temp = temp.concat(e)
+          } else if (item.link) {
+            temp.pop()
+          }
+        }
+      }
+
+      return temp
+    }
+  }
+}
