+
+    clinicList[i]    =   {
+        aliasDate       :   month + '-' + day ,
+        diagnoseDate    :   year + '-' + month + '-' + day ,
+        week            :   weeks[curr.getDay()],
+        stamp           :   stamp
+
+    }
+
+}
+
+export default clinicList
